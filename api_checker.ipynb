      "\u001b[0m            \u001b[1;32mraise\u001b[0m \u001b[0mNotImplementedError\u001b[0m\u001b[1;33m(\u001b[0m\u001b[0mmsg\u001b[0m\u001b[1;33m)\u001b[0m\u001b[1;33m\n",
      "\u001b[0m\u001b[1;33m\n",
      "\u001b[0m        \u001b[1;31m# scaling factor for HiDPI displays, `None` until initialized\u001b[0m\u001b[1;33m\n",
      "\u001b[0m        \u001b[0mself\u001b[0m\u001b[1;33m.\u001b[0m\u001b[0m_contentScaleFactor\u001b[0m \u001b[1;33m=\u001b[0m \u001b[1;32mNone\u001b[0m\u001b[1;33m\n",
      "\u001b[0m\u001b[1;33m\n",
      "\u001b[0m        \u001b[1;31m# Code to allow iohub to know id of any psychopy windows created\u001b[0m\u001b[1;33m\n",
      "\u001b[0m        \u001b[1;31m# so kb and mouse event filtering by window id can be supported.\u001b[0m\u001b[1;33m\n",
      "\u001b[0m        \u001b[1;31m#\u001b[0m\u001b[1;33m\n",
      "\u001b[0m        \u001b[1;31m# If an iohubConnection is active, give this window os handle to\u001b[0m\u001b[1;33m\n",
