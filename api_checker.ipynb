      "\u001b[0m\u001b[1;33m\n",
      "\u001b[0m            \u001b[1;31m# light object to modify\u001b[0m\u001b[1;33m\n",
      "\u001b[0m            \u001b[0mlight\u001b[0m \u001b[1;33m=\u001b[0m \u001b[0mself\u001b[0m\u001b[1;33m.\u001b[0m\u001b[0m_lights\u001b[0m\u001b[1;33m[\u001b[0m\u001b[0mindex\u001b[0m\u001b[1;33m]\u001b[0m\u001b[1;33m\n",
      "\u001b[0m\u001b[1;33m\n",
